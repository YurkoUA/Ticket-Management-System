{
  "compilerOptions": {
    "module": "none",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "target": "es5",
    "inlineSourceMap": true,
    "inlineSources": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "allowSyntheticDefaultImports": true,
    "outDir": "Scripts/AutoGenerated"
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}
